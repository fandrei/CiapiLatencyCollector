<?xml version="1.0"?>
<configuration>

  <system.web>
    <compilation debug="true" targetFramework="4.0" />

    <authentication mode="None" />

    <authorization>
      <deny users="?" />
    </authorization>
  </system.web>

  <system.webServer>
    <modules>
      <add name="CustomBasicAuthenticationModule" type="LatencyCollectorConfigSite.BasicAuthenticationModule"/>
    </modules>
  </system.webServer>

  <location path="CreateUser.aspx">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
</configuration>
